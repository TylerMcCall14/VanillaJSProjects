{
    "name": "Today's Jump-Start",
    "description": "Get a jump start on your day with this planner",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html"
      },
      "icons": {
        "16": "/images/icons16.png",
        "32": "/images/icons32.png",
        "48": "/images/icons48.png",
        "128": "/images/icons128.png"
      },
      "permissions": [
        "storage",
        "tabs",
        "contextMenus"
      ],
      "background":{
        "scripts": [
          "js/action-items-utils.js",
          "packages/uuidv4.min.js",
          "js/background.js"
        ],
        "persistent": false
      },
    "manifest_version": 2
  }